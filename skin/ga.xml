    <layer name="tplugin" 
           url="%SWFPATH%/plugins/thumbnails.swf" 
           alturl="%SWFPATH%/plugins/thumbnails.js" 
           preload="true" 
           onresized="resizethumbs();showimage-onloaded();"/>
    
    

    <layer name="sacphong_btn" keep="true" parent="root_layer" align="leftcenter" 
           scale="1" x="0" y="0" y.touch="45" url=""  zorder="99"
           style="skin_tooltips" tooltip="" tooltip_vi="" tooltip_en="" visible="false"
           onover2="set(layer[skin_tooltip].ox,80);set(layer[skin_tooltip].oy,0);"
           onout2="wait(0.1);set(layer[skin_tooltip].ox,0);set(layer[skin_tooltip].oy,60);"
    />
    <!--=======================-->

    <layer name="bg_gallery_fs" parent="root_layer" type="image" keep="true" align="center" url="" x="0" y="45" width="100%" height="100%" 
           autoalpha="true" zorder="99" 
           onclick="closegallery();" alpha="0" visible="false"/>

    

    <action name="linkgallery_action">
        set(autorotate.enabled,false); 
        set(layer[sacphong_btn].visible,false);
        set(layer[bg_gallery_fs].visible,true);
        set(layer[bg_gallery_fs].alpha,1);
        showgallery(get(layer[scene].namegallery));
    </action>

    <events name="btn_event_gallery" keep="true"
            onloadcomplete="
          if(layer[scene].btn_sp,
            Sp_action();
          );

          if(layer[scene].linkgallery,
            linkgallery_action();
          );

          if(!layer[scene].btn_sp AND !layer[scene].linkgallery,
            set(layer[sacphong_btn].visible,false);
            set(layer[bg_gallery_fs].visible,false);
            );
      "
    />
    
</krpano>
